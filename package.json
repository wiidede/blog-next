{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.6.7",
  "scripts": {
    "build": "esno ./scripts/rss.ts && cross-env NODE_OPTIONS=--max-old-space-size=4096 vite-ssg build",
    "dev": "vite --port 3333 --open --host",
    "lint": "eslint . --ext .vue,.ts,.html --fix",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@unhead/vue": "^1.1.30",
    "@unocss/reset": "^0.53.5",
    "@vueuse/core": "^10.2.1",
    "@vueuse/head": "^1.1.26",
    "dayjs": "^1.11.9",
    "nprogress": "^0.2.0",
    "typed.js": "^2.0.16",
    "valine": "^1.5.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.7",
    "@iconify-json/carbon": "^1.1.18",
    "@iconify-json/logos": "^1.1.33",
    "@iconify/utils": "^2.1.7",
    "@types/fs-extra": "^11.0.1",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^20.4.1",
    "@types/nprogress": "^0.2.0",
    "@unocss/eslint-config": "^0.53.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "cross-env": "^7.0.3",
    "enso": "1.0.0-alpha.3",
    "eslint": "^8.44.0",
    "feed": "^4.2.2",
    "fs-extra": "^11.1.1",
    "https-localhost": "^4.7.1",
    "lint-staged": "^13.2.3",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.9.0",
    "pnpm": "^8.6.7",
    "shiki": "^0.14.3",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.11.2",
    "typescript": "^5.1.6",
    "unocss": "^0.53.5",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.2",
    "vite-bundle-visualizer": "^0.8.0",
    "vite-plugin-inspect": "^0.7.32",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-vue-devtools": "^0.4.14",
    "vite-plugin-vue-markdown": "^0.23.5",
    "vite-plugin-webfont-dl": "^3.7.6",
    "vite-ssg": "^0.23.0",
    "vite-ssg-sitemap": "^0.5.1",
    "vue-router-better-scroller": "^0.0.0",
    "vue-tsc": "^1.8.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.vue": "eslint --fix",
    "*.md": "eslint --fix",
    "*.html": "eslint --fix"
  }
}
