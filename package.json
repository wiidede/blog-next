{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "scripts": {
    "build": "esno ./scripts/rss.ts && cross-env NODE_OPTIONS=--max-old-space-size=4096 vite-ssg build",
    "dev": "vite --port 3333 --open --host",
    "lint": "eslint .",
    "fix-case": "npx case-police --fix",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@unhead/vue": "^1.8.8",
    "@unocss/reset": "^0.58.0",
    "@vueuse/core": "^10.7.0",
    "@vueuse/head": "^2.0.0",
    "dayjs": "^1.11.10",
    "markdown-it-table-of-contents": "^0.6.0",
    "nprogress": "^0.2.0",
    "typed.js": "^2.1.0",
    "unplugin-vue-markdown": "^0.25.2",
    "valine": "^1.5.1",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.2",
    "@iconify-json/carbon": "^1.1.24",
    "@iconify-json/logos": "^1.1.40",
    "@iconify/utils": "^2.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/markdown-it-link-attributes": "^3.0.4",
    "@types/node": "^20.10.4",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "^0.58.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "enso": "1.0.0-alpha.3",
    "eslint": "^8.55.0",
    "eslint-plugin-case-police": "^0.6.1",
    "feed": "^4.2.2",
    "fs-extra": "^11.2.0",
    "https-localhost": "^4.7.1",
    "lint-staged": "^15.2.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.9.0",
    "pnpm": "^8.11.0",
    "shiki": "^0.14.6",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.6",
    "vite-bundle-visualizer": "^0.11.0",
    "vite-plugin-inspect": "^0.8.1",
    "vite-plugin-pages": "^0.32.0",
    "vite-plugin-vue-devtools": "^0.5.1",
    "vite-plugin-webfont-dl": "^3.9.1",
    "vite-ssg": "^0.23.5",
    "vite-ssg-sitemap": "^0.6.1",
    "vue-router-better-scroller": "^0.0.0",
    "vue-tsc": "^1.8.25"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm run lint --fix"
  }
}
